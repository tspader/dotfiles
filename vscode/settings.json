{
    "extensions.ignoreRecommendations": true,
    "security.workspace.trust.untrustedFiles": "open",
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "window.zoomLevel": -1,
    "editor.tabSize": 2,
    "editor.renderLineHighlight": "all",
    "workbench.colorCustomizations": {
        "editor.lineHighlightBackground": "#00000000",
        "editor.lineHighlightBorder": "#0000ff"
    },
    "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?_",
    "files.associations": {
        "*.vertex": "glsl",
        "*.fragment": "glsl",
        "*.glsl": "glsl"
    },
    "C_Cpp.dimInactiveRegions": false,
    "explorer.confirmDragAndDrop": false,
    "git.openRepositoryInParentFolders": "never",
    "debug.onTaskErrors": "abort",
    "terminal.integrated.defaultProfile.linux": "bash",
    "terminal.integrated.defaultProfile.windows": "PowerShell",
    "cmake.showOptionsMovedNotification": false,
    "cmake.pinnedCommands": [
        "workbench.action.tasks.configureTaskRunner",
        "workbench.action.tasks.runTask"
		],
    "editor.indentSize": "tabSize",
    "editor.detectIndentation": false,
    "python.createEnvironment.trigger": "off",
    "pylint.enabled": false,
    "terminal.integrated.fontSize": 18,
    "editor.cursorStyle": "block",
    "terminal.integrated.cursorBlinking": true, 
    "editor.find.seedSearchStringFromSelection": "never",
    "terminal.integrated.tabs.defaultIcon": "chevron-right",
    "workbench.activityBar.location": "top",
    "vanim.ranges": [
      { "start": 0,   "end": 3 },
      { "start": 10,  "end": 14 },
      { "start": 25,  "end": 30 }
    ],
    "editor.autoClosingBrackets": "never",
    "editor.autoClosingComments": "never",
    "editor.autoClosingDelete": "never",
    "editor.minimap.enabled": false,
    "chat.commandCenter.enabled": false,
    "diffEditor.renderSideBySide": false,
    "diffEditor.experimental.useTrueInlineView": true,
    "editor.accessibilitySupport": "off",
    "accessibility.dimUnfocused.enabled": true,
    "accessibility.dimUnfocused.opacity": 0.6,
    "editor.peekWidgetDefaultFocus": "editor",
    "explorer.fileNesting.patterns": {
      "*.ts": "${capture}.js",
      "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
      "*.jsx": "${capture}.js",
      "*.tsx": "${capture}.ts",
      "tsconfig.json": "tsconfig.*.json",
      "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb, bun.lock",
      "*.sqlite": "${capture}.${extname}-*",
      "*.db": "${capture}.${extname}-*",
      "*.sqlite3": "${capture}.${extname}-*",
      "*.db3": "${capture}.${extname}-*",
      "*.sdb": "${capture}.${extname}-*",
      "*.s3db": "${capture}.${extname}-*"
    },
    "window.customMenuBarAltFocus": false,
    "remote.SSH.remotePlatform": {
      "aral": "windows"
    },
    "yaml.schemas": {
      "file:///c%3A/Users/ts185327/.vscode/extensions/atlassian.atlascode-3.8.8/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
    }
  }